    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-jest": "^27.4.6",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^27.0.7",
    "rxjs": "^6.5.4",
    "rxjs-for-await": "0.0.2",
    "ts-jest": "^27.1.5",
    "tsup": "^6.2.3",
    "typescript": "^4.0.3",
    "yargs-parser": "20.0.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
